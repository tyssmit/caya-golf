<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        tools:context=".Login"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="299dp"
            android:layout_height="184dp"
            android:layout_marginTop="8dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.142"
            app:srcCompat="@drawable/cayagolf_logo_200_60" />

        <Button
            android:id="@+id/logout"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="84dp"
            android:layout_marginBottom="104dp"
            android:background="#22b8f0"
            android:onClick="logout"
            android:text="LOGOUT"
            android:textColor="#FFFFFF"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/userProfile" />

        <Button
            android:id="@+id/postButton"
            android:layout_width="97dp"
            android:layout_height="63dp"
            android:background="#2e4f6e"
            android:text="POST"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="334dp" />

        <Button
            android:id="@+id/scrambleButton"
            android:layout_width="97dp"
            android:layout_height="63dp"
            android:background="#22b8f0"
            android:text="SCRAMBLE"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="258dp"
            tools:layout_editor_absoluteY="426dp" />

        <Button
            android:id="@+id/teamButton"
            android:layout_width="97dp"
            android:layout_height="63dp"
            android:background="#2e4f6e"
            android:text="TEAM"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="157dp"
            tools:layout_editor_absoluteY="426dp" />

        <Button
            android:id="@+id/soloButton"
            android:layout_width="97dp"
            android:layout_height="63dp"
            android:background="#22b8f0"
            android:text="solo"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="56dp"
            tools:layout_editor_absoluteY="426dp" />

        <Button
            android:id="@+id/scoreButton"
            android:layout_width="97dp"
            android:layout_height="63dp"
            android:background="#22b8f0"
            android:text="SCORE"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="157dp"
            tools:layout_editor_absoluteY="334dp" />

        <Button
            android:id="@+id/cayaButton"
            android:layout_width="97dp"
            android:layout_height="63dp"
            android:background="#2e4f6e"
            android:text="CAYA"
            android:textColor="#FFFFFF"
            tools:layout_editor_absoluteX="258dp"
            tools:layout_editor_absoluteY="334dp" />

        <Button
            android:id="@+id/userProfile"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="#2e4f6e"
            android:text="MY ACCOUNT"
            android:textColor="#FFFFFF"
            app:layout_constraintEnd_toEndOf="@+id/scrambleButton"
            app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintStart_toStartOf="@+id/soloButton"
            tools:layout_editor_absoluteY="526dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>